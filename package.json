{
  "name": "ptz-core-domain",
  "version": "1.5.0",
  "description": "Polutz Core Domain.",
  "main": "./dist/index.js",
  "typings": "./src/index.ts",
  "scripts": {
    "lint": "tslint ./src/**/*.ts ./src/**/*.test.ts ./src/**/*.d.ts",
    "js": "rimraf dist-es6 && tsc && rimraf dist && babel dist-es6 -d dist --presets es2015 --source-maps",
    "pretest": "npm-run-all --parallel js lint",
    "mocha": "mocha ./dist/**/*.js --require babel-polyfill",
    "test": "nyc npm run mocha && nyc report --reporter=html --reporter=text-lcov > coverage.lcov && f() { EXIT=0; codecov --token=cbef0920-fdc4-4cc3-8d52-51a0298afea3 || exit $EXIT; }; f",
    "test:cover": "npm run test && opn ./coverage/lcov-report/index.html",
    "predebug": "npm run pretest",
    "debug": "node --nolazy --debug-brk=5858 dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polutz/ptz-core-domain.git"
  },
  "author": "angeloocana.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/polutz/ptz-core-domain/issues"
  },
  "homepage": "https://github.com/polutz/ptz-core-domain#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "ptz-assert": "^1.6.5",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "tslint": "^4.5.1",
    "typescript": "^2.2.2",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "opn-cli": "^3.1.0",
    "@types/shortid": "0.0.28"
  },
  "dependencies": {
    "ptz-log": "^1.0.0",
    "ptz-validations": "^1.0.0",
    "shortid": "^2.2.6"
  },
  "scriptsBK": {
    "lint": "tslint ./src/**/*.ts ./src/**/*.test.ts ./src/**/*.d.ts",
    "js": "tsc && babel dist -d dist --presets es2015",
    "pretest": "npm-run-all --parallel js lint",
    "mocha": "mocha ./dist/**/*.js --require babel-polyfill",
    "test": "nyc npm run mocha && nyc report --reporter=text-lcov > coverage.lcov && codecov --token=cbef0920-fdc4-4cc3-8d52-51a0298afea3",
    "predebug": "npm run pretest",
    "debug": "node --nolazy --debug-brk=5858 dist/index.js"
  }
}
